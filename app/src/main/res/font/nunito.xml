<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Light -->
    <font
        android:fontStyle="normal"
        android:fontWeight="300"
        android:font="@font/nunito_light"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="300"
        android:font="@font/nunito_light_italic"/>

    <!-- Regular -->
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/nunito_regular"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/nunito_italic"/>

    <!-- Medium -->
    <font
        android:fontStyle="normal"
        android:fontWeight="500"
        android:font="@font/nunito_medium"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="500"
        android:font="@font/nunito_medium_italic"/>

    <!-- SemiBold -->
    <font
        android:fontStyle="normal"
        android:fontWeight="600"
        android:font="@font/nunito_semi_bold"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="600"
        android:font="@font/nunito_semi_bold_italic"/>

    <!-- Bold -->
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/nunito_bold"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="700"
        android:font="@font/nunito_bold_italic"/>

    <!-- ExtraBold -->
    <font
        android:fontStyle="normal"
        android:fontWeight="800"
        android:font="@font/nunito_extra_bold"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="800"
        android:font="@font/nunito_extra_bold_italic"/>

    <!-- Black -->
    <font
        android:fontStyle="normal"
        android:fontWeight="900"
        android:font="@font/nunito_black"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="900"
        android:font="@font/nunito_black_italic"/>

</font-family>
